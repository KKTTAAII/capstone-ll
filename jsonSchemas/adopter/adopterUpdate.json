{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "hjbfyeb"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "bhdskjgnrekjn"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "kratai@gmail.com"
            ]
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "hello world"
            ]
        },
        "privateOutdoors": {
            "type": "boolean",
            "default": false,
            "title": "The privateOutdoors Schema",
            "examples": [
                false
            ]
        },
        "numOfDogs": {
            "type": "integer",
            "default": 0,
            "title": "The numOfDogs Schema",
            "examples": [
                2
            ]
        },
        "preferredGender": {
            "type": "string",
            "default": "",
            "title": "The preferredGender Schema",
            "examples": [
                "Male"
            ]
        },
        "preferredAge": {
            "type": "string",
            "default": "",
            "title": "The preferredAge Schema",
            "examples": [
                "young"
            ]
        }
    },
    "examples": [{
        "username": "hjbfyeb",
        "password": "bhdskjgnrekjn",
        "email": "kratai@gmail.com",
        "description": "hello world",
        "privateOutdoors": false,
        "numOfDogs": 2,
        "preferredGender": "Male",
        "preferredAge": "young"
    }]
}