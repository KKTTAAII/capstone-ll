{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "username",
        "password",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "title": "The username Schema",
            "examples": [
                "kratai"
            ]
        },
        "password": {
            "type": "string",
            "title": "The password Schema",
            "examples": [
                "bhdskjgnrekjn"
            ]
        },
        "email": {
            "type": "string",
            "minLength": 6,
            "maxLength": 60,
            "format": "email",
            "examples": [
                "kratai@gmail.com"
            ]
        },
        "picture": {
            "type": "object",
            "default": "",
            "title": "The picture Schema"
        },
        "description": {
            "type": "string",
            "default": "",
            "title": "The description Schema",
            "examples": [
                "hello world"
            ]
        },
        "privateOutdoors": {
            "type": "boolean",
            "default": false,
            "title": "The privateOutdoors Schema",
            "examples": [
                false
            ]
        },
        "numOfDogs": {
            "type": "integer",
            "default": 0,
            "title": "The numOfDogs Schema",
            "examples": [
                2
            ]
        },
        "preferredGender": {
            "type": "string",
            "default": "",
            "title": "The preferredGender Schema",
            "examples": [
                "Male"
            ]
        },
        "preferredAge": {
            "type": "string",
            "default": "",
            "title": "The preferredAge Schema",
            "examples": [
                "young"
            ]
        }
    }
}