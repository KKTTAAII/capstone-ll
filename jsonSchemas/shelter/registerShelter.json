{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "username",
        "password",
        "name",
        "city",
        "state",
        "phoneNumber",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "title": "The username Schema",
            "examples": [
                "kratai"
            ]
        },
        "password": {
            "type": "string",
            "title": "The password Schema",
            "examples": [
                "bhdskjgnrekjn"
            ]
        },
        "name": {
            "type": "string",
            "title": "The name Schema",
            "examples": [
                "dog lovers"
            ]
        },
        "address": {
            "type": "string",
            "title": "The address Schema",
            "examples": [
                "303 overlook Dr."
            ]
        },
        "city": {
            "type": "string",
            "title": "The city Schema",
            "examples": [
                "Breckenridge"
            ]
        },
        "state": {
            "type": "string",
            "title": "The state Schema",
            "examples": [
                "CO"
            ]
        },
        "postcode": {
            "type": "string",
            "title": "The postcode Schema",
            "examples": [
                "80424"
            ]
        },
        "phoneNumber": {
            "type": "string",
            "title": "The phoneNumber Schema",
            "examples": [
                "028017562"
            ]
        },
        "email": {
            "type": "string",
            "format": "email",
            "examples": [
                "kratai@gmail.com"
            ]
        },
        "description": {
            "type": "string",
            "title": "The description Schema",
            "examples": [
                "hello world"
            ]
        }
    },
    "examples": [{
        "username": "kratai",
        "password": "bhdskjgnrekjn",
        "name": "dog lovers",
        "address": "303 yellowLine Dr.",
        "city": "Grand Junction",
        "state": "CO",
        "postcode": "80424",
        "phoneNumber": "028017562",
        "email": "kratai@gmail.com",
        "description": "hello world"
    }]
}