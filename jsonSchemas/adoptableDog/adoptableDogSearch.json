{
    "$schema": "http://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Coco"
            ]
        },
        "breedId": {
            "type": "integer",
            "default": 0,
            "title": "The breedId Schema",
            "examples": [
                130
            ]
        },
        "gender": {
            "type": "string",
            "default": "",
            "title": "The gender Schema",
            "examples": [
                "Male"
            ]
        },
        "age": {
            "type": "string",
            "default": "",
            "title": "The age Schema",
            "examples": [
                "Adult"
            ]
        },
        "goodWKids": {
            "type": "boolean",
            "default": false,
            "title": "The goodWKids Schema",
            "examples": [
                true
            ]
        },
        "goodWDogs": {
            "type": "boolean",
            "default": false,
            "title": "The goodWDogs Schema",
            "examples": [
                true
            ]
        },
        "goodWCats": {
            "type": "boolean",
            "default": false,
            "title": "The goodWCats Schema",
            "examples": [
                false
            ]
        }
    },
    "examples": [{
        "name": "Coco",
        "breedId": 130,
        "gender": "Male",
        "age": "Adult",
        "goodWKids": true,
        "goodWDogs": true,
        "goodWCats": false
    }]
}